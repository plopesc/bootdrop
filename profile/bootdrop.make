; Core version
core = 7.x

; Drush Make API version
api = 2

; Set contrib directory.
defaults[projects][subdir] = "contrib"

; Base installation profile
includes[base] = "make/base.make"

; Development
;includes[development] = "make/development.make"

; Theme
includes[theming] = "make/theming.make"

; Images
includes[images] = "make/images.make"

; Multilingual
;includes[i18n] = "make/i18n.make"

; Commerce
;includes[commerce] = "make/commerce.make"



